model ProductOption {
  id                 Int                 @id @default(autoincrement())
  productId          Int                 @map("product_id")
  optionValue1Id     Int                 @map("option_value1_id")
  optionValue2Id     Int?                @map("option_value2_id")
  optionValue3Id     Int?                @map("option_value3_id")
  optionValue4Id     Int?                @map("option_value4_id")
  optionValue5Id     Int?                @map("option_value5_id")
  isActive           Boolean             @default(true) @map("is_active")
  taobaoSkuId        String              @map("taobao_sku_id") @db.VarChar(30)
  priceCny           Float               @map("price_cny") @db.Float
  price              Int
  stock              Int?
  optionString       String              @map("option_string") @db.VarChar(100)
  defaultShippingFee Int?                @map("default_shipping_fee")
  productOption1     ProductOptionValue  @relation("product_option_option_value1_idToproduct_option_value", fields: [optionValue1Id], references: [id])
  productOption2     ProductOptionValue? @relation("product_option_option_value2_idToproduct_option_value", fields: [optionValue2Id], references: [id])
  productOption3     ProductOptionValue? @relation("product_option_option_value3_idToproduct_option_value", fields: [optionValue3Id], references: [id])
  productOption4     ProductOptionValue? @relation("product_option_option_value4_idToproduct_option_value", fields: [optionValue4Id], references: [id])
  productOption5     ProductOptionValue? @relation("product_option_option_value5_idToproduct_option_value", fields: [optionValue5Id], references: [id])
  product            Product             @relation(fields: [productId], references: [id])

  @@unique([productId, taobaoSkuId], name: "UQ_product_id_sku_id")
  @@unique([optionValue1Id, optionValue2Id, optionValue3Id, optionValue4Id, optionValue5Id], name: "UQ_product_option")
  @@index([optionValue2Id], name: "FK_product_option_option_value2_id_product_option_value_id")
  @@index([optionValue3Id], name: "FK_product_option_option_value3_id_product_option_value_id")
  @@index([optionValue4Id], name: "FK_product_option_option_value4_id_product_option_value_id")
  @@index([optionValue5Id], name: "FK_product_option_option_value5_id_product_option_value_id")
  @@map("product_option")
}

model ProductOptionName {
  id                 Int                  @id @default(autoincrement())
  productId          Int                  @map("product_id")
  order              Int                  @db.TinyInt
  name               String               @db.VarChar(200)
  taobaoPid          String               @map("taobao_pid") @db.VarChar(20)
  isNameTranslated   Boolean              @default(false) @map("is_name_translated")
  hasImage           Boolean              @map("has_image")
  isActive           Boolean?             @default(true) @map("is_active")
  product            Product              @relation("productToproduct_option_name", fields: [productId], references: [id])
  productOptionValue ProductOptionValue[]

  @@index([productId], name: "FK_product_option_name_product_id_product_id")
  @@map("product_option_name")
}

model ProductOptionValue {
  id                  Int               @id @default(autoincrement())
  productOptionNameId Int               @map("product_option_name_id")
  optionNameOrder     Int               @map("option_name_order") @db.TinyInt
  name                String            @db.VarChar(200)
  taobaoVid           String            @map("taobao_vid") @db.VarChar(20)
  image               String?           @db.VarChar(500)
  number              Int               @db.SmallInt
  isNameTranslated    Boolean           @default(false) @map("is_name_translated")
  isActive            Boolean           @default(true) @map("is_active")
  originalName        String?           @db.VarChar(200)
  productOptionName   ProductOptionName @relation(fields: [productOptionNameId], references: [id])
  optionValue1        ProductOption[]   @relation("product_option_option_value1_idToproduct_option_value")
  optionValue2        ProductOption[]   @relation("product_option_option_value2_idToproduct_option_value")
  optionValue3        ProductOption[]   @relation("product_option_option_value3_idToproduct_option_value")
  optionValue4        ProductOption[]   @relation("product_option_option_value4_idToproduct_option_value")
  optionValue5        ProductOption[]   @relation("product_option_option_value5_idToproduct_option_value")

  @@index([productOptionNameId], name: "FK_product_option_value_pon_id_product_option_name_id")
  @@map("product_option_value")
}

model ProductStore {
  id                Int               @id @default(autoincrement())
  userId            Int               @map("user_id")
  productId         Int               @map("product_id")
  state             Int               @db.TinyInt
  storeProductId    String?           @map("store_product_id") @db.VarChar(100)
  etcVendorItemId   String?           @map("etc_vendor_item_id") @db.VarChar(20)
  siteCode          String            @map("site_code") @db.VarChar(10)
  storeUrl          String?           @map("store_url") @db.VarChar(200)
  connectedAt       DateTime          @default(now()) @map("connected_at") @db.DateTime(0)
  cnt               Int               @default(0)
  inflow            Int?              @default(0)
  product           Product           @relation("productToproduct_store", fields: [productId], references: [id])
  productStoreState ProductStoreState @relation("product_storeToproduct_store_state", fields: [state], references: [id])
  user              User              @relation("product_storeTouser", fields: [userId], references: [id])
  productViewLog    productViewLog[]  @relation("productViewLogToproduct_store")
  productStoreLog   ProductStoreLog[] @relation("product_storeToproduct_store_log")

  @@index([productId], name: "FK_product_store_product_id_product_id")
  @@index([state], name: "FK_product_store_state_product_store_state_id")
  @@index([userId], name: "FK_product_store_user_id_user_id")
  @@map("product_store")
}

