  streetApiKey2                  String  @db.VarChar(200)
  streetApiKey3                  String  @db.VarChar(200)
  streetApiKey4                  String  @db.VarChar(200)
  streetNormalApiKey2            String  @db.VarChar(200)
  streetNormalApiKey3            String  @db.VarChar(200)
  streetNormalApiKey4            String  @db.VarChar(200)
  streetNormalUseKeyType         String  @default("1") @db.VarChar(10)
  streetUseKeyType               String  @default("1") @db.VarChar(10)
  streetApiMemo                  String  @db.VarChar(45)
  streetApiMemo2                 String  @db.VarChar(45)
  streetApiMemo3                 String  @db.VarChar(45)
  streetApiMemo4                 String  @db.VarChar(45)
  streetNormalApiMemo            String  @db.VarChar(45)
  streetNormalApiMemo2           String  @db.VarChar(45)
