  coupangUseType                 String  @default("Y") @map("coupang_use_type") @db.VarChar(10)
  streetNormalUseType            String  @default("Y") @map("street_normal_use_type") @db.VarChar(10)
  gmarketUseType                 String  @default("Y") @map("gmarket_use_type") @db.VarChar(10)
  auctionUseType                 String  @default("Y") @map("auction_use_type") @db.VarChar(10)
  interparkUseType               String  @default("Y") @map("interpark_use_type") @db.VarChar(10)
  wemakepriceUseType             String  @default("Y") @map("wemakeprice_use_type") @db.VarChar(10)
  lotteonNormalUseType           String  @default("Y") @map("lotteon_normal_use_type") @db.VarChar(10)
  tmonUseType                    String  @default("Y") @map("tmon_use_type") @db.VarChar(10)
  lotteonSellerType              String  @default("G") @map("lotteon_seller_type") @db.VarChar(10)
  interparkEditCertKey           String  @map("interpark_edit_cert_key") @db.VarChar(200)
